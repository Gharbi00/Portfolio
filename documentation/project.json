{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/documentation/src",
  "prefix": "sifca-monorepo",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/documentation",
        "index": "apps/documentation/src/index.html",
        "main": "apps/documentation/src/main.ts",
        "polyfills": "apps/documentation/src/polyfills.ts",
        "tsConfig": "apps/documentation/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/documentation/src/favicon.ico",
          "apps/documentation/src/assets"
        ],
        "styles": [
          "apps/documentation/src/assets/css/custom-animation.css",
          "apps/documentation/src/assets/css/framework.css",
          "apps/documentation/src/assets/css/responsive.css",
          "apps/documentation/src/assets/css/style.css",
          "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
          "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
          "node_modules/aos/dist/aos.css",
          "node_modules/bootstrap/dist/css/bootstrap.min.css",
          "apps/documentation/src/styles.scss"
        ],
        "scripts": [
          "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "50mb",
              "maximumError": "50mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50mb",
              "maximumError": "50mb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/documentation/src/environments/environment.ts",
              "with": "apps/documentation/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "docs:build:production"
        },
        "development": {
          "browserTarget": "docs:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "docs:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/documentation/**/*.ts",
          "apps/documentation/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/documentation/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}