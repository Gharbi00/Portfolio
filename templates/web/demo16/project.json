{
  "name": "demo16",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/clients/templates/web/demo16/src",
  "prefix": "sifca-monorepo",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/clients/templates/web/demo16/browser",
        "index": "apps/clients/templates/web/demo16/src/index.html",
        "main": "apps/clients/templates/web/demo16/src/main.ts",
        "polyfills": "apps/clients/templates/web/demo16/src/polyfills.ts",
        "tsConfig": "apps/clients/templates/web/demo16/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "allowedCommonJsDependencies": ["ngx-lightbox", "subscriptions-transport-ws"],
        "assets": [
          "apps/clients/templates/web/demo16/src/favicon.ico",
          "apps/clients/templates/web/demo16/src/assets"
        ],
        "styles": [
          "apps/clients/templates/web/demo12/src/styles.scss",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/bootstrap.min.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/font-awesome.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/flaticon/flaticon.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/slick.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/slick-theme.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/jquery-ui.min.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/sal.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/magnific-popup.css",
          "apps/clients/templates/web/demo12/src/assets/css/vendor/base.css",
          "apps/clients/templates/web/demo12/src/assets/css/style.min.css",
          "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
          "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css"
        ],
        "scripts": [
          "apps/clients/templates/web/demo12/src/assets/js/vendor/modernizr.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/jquery.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/popper.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/bootstrap.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/slick.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/js.cookie.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/jquery-ui.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/jquery.ui.touch-punch.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/jquery.countdown.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/sal.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/jquery.magnific-popup.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/imagesloaded.pkgd.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/isotope.pkgd.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/counterup.js",
          "apps/clients/templates/web/demo12/src/assets/js/vendor/waypoints.min.js",
          "apps/clients/templates/web/demo12/src/assets/js/main.js"
        ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/clients/templates/web/demo16/src/environments/environment.ts",
              "with": "apps/clients/templates/web/demo16/src/environments/environment.prod.ts"
            }
          ],
          "optimization": {
            "scripts": true,
            "styles": true,
            "fonts": false
          },
          "aot": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "120mb",
              "maximumError": "140mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "120mb",
              "maximumError": "140mb"
            }
          ]
        },
        "sandbox": {
          "fileReplacements": [
            {
              "replace": "apps/clients/templates/web/demo16/src/environments/environment.ts",
              "with": "apps/clients/templates/web/demo16/src/environments/environment.sandbox.ts"
            }
          ],
          "optimization": {
            "scripts": true,
            "styles": true,
            "fonts": false
          },
          "aot": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "120mb",
              "maximumError": "140mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "120mb",
              "maximumError": "140mb"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "demo16:build:production"
        },
        "development": {
          "browserTarget": "demo16:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "demo16:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "**/*.ts",
          "**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "jest.config.ts",
        "passWithNoTests": true
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/clients/templates/web/demo16/server",
        "main": "apps/clients/templates/web/demo16/src/ssr.server.ts",
        "tsConfig": "apps/clients/templates/web/demo16/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "apps/clients/templates/web/demo16/src/environments/environment.ts",
              "with": "apps/clients/templates/web/demo16/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "sourceMap": false,
          "extractLicenses": true
        },
        "sandbox": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "apps/clients/templates/web/demo16/src/environments/environment.ts",
              "with": "apps/clients/templates/web/demo16/src/environments/environment.sandbox.ts"
            }
          ],
          "optimization": true,
          "sourceMap": false,
          "extractLicenses": true
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "demo16:build:development",
          "serverTarget": "demo16:server:development"
        },
        "production": {
          "browserTarget": "demo16:build:production",
          "serverTarget": "demo16:server:production"
        }
      },
      "defaultConfiguration": "development"
    }
  },
  "tags": []
}
